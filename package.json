{
  "private": false,
  "name": "@standard-software/parts",
  "version": "7.6.0",
  "description": "JavaScript Code Parts",
  "main": "./release/parts.js",
  "scripts": {
    "babel":              "babel --config-file ./config/babel.config.js ./source --out-dir ./build/babel_webpack/babel",
    "webpack":            "webpack --config ./config/webpack.config.js",
    "webpack_testcode":   "webpack --config ./config/webpack_testcode.config.js",
    "copy:release":       "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/parts.js', './release/parts.js')\"",
    "clear:babel":        "node -e \"require('fs-extra').removeSync('./build/babel_webpack/babel/')\"",
    "clear:webpack":      "node -e \"require('fs-extra').removeSync('./build/babel_webpack/webpack/')\"",
    "clear:build":        "npm run clear:babel && npm run clear:webpack",
    "build:babel":        "npm run clear:build && npm run babel",
    "build:webpack":      "npm run webpack && npm run webpack_testcode",
    "test:source:deno:1": "deno run --allow-write ./test/source/test_source_deno_1.js",
    "test:source:deno:2": "deno run --allow-write ./test/source/test_source_deno_2.js",
    "test:source:deno:3": "deno run --allow-write ./test/source/test_source_deno_3.js",
    "test:source:deno":   "npm run test:source:deno:1 && npm run test:source:deno:2 && npm run test:source:deno:3",
    "test:babel:node":    "node ./test/build/babel_webpack/babel/test_babel_webpack_babel_1.js && node ./test/build/babel_webpack/babel/test_babel_webpack_babel_2.js",
    "test:webpack:node":  "node ./test/build/babel_webpack/webpack/test_babel_webpack_webpack_1.js && node ./test/build/babel_webpack/webpack/test_babel_webpack_webpack_2.js",
    "test:webpack:wsh":   "cscript ./test/release/wsh/test.wsf",
    "start":              "npm run test:source:deno && npm run build:babel && npm run test:babel:node && npm run build:webpack && npm run test:webpack:node && npm run copy:release",
    "start:wsh":          "npm start && npm run test:webpack:wsh",
    "start:jest":         "npm start && jest",
    "test:source:deno:watch": "npm-watch test:source:deno",
    "start:watch":        "npm-watch start",
    "eslint":             "eslint",
    "eslint:fix":         "eslint --fix ./source/*.js ./source/*/*.js",
    "jest":               "jest",
    "jest:watch":         "jest --watchAll",
    "_copy:sheet:rhino:parts":      "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/parts.js', './test/release/clasp/test_parts_spreadsheet_Rhino/parts.js')\"",
    "_copy:sheet:rhino:test_parts": "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/test_parts.js', './test/release/clasp/test_parts_spreadsheet_Rhino/test_parts.js')\"",
    "copy:sheet:rhino":             "npm run _copy:sheet:rhino:parts && npm run _copy:sheet:rhino:test_parts",
    "_copy:sheet:v8:parts":         "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/parts.js', './test/release/clasp/test_parts_spreadsheet_v8/parts.js')\"",
    "_copy:sheet:v8:test_parts":    "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/test_parts.js', './test/release/clasp/test_parts_spreadsheet_v8/test_parts.js')\"",
    "copy:sheet:v8":                "npm run _copy:sheet:v8:parts && npm run _copy:sheet:v8:test_parts",
    "_copy:standalone:rhino:parts":      "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/parts.js', './test/release/clasp/test_parts_standalone_Rhino/parts.js')\"",
    "_copy:standalone:rhino:test_parts": "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/test_parts.js', './test/release/clasp/test_parts_standalone_Rhino/test_parts.js')\"",
    "copy:standalone:rhino":             "npm run _copy:standalone:rhino:parts && npm run _copy:standalone:rhino:test_parts",
    "_copy:standalone:v8:parts":         "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/parts.js', './test/release/clasp/test_parts_standalone_v8/parts.js')\"",
    "_copy:standalone:v8:test_parts":    "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/test_parts.js', './test/release/clasp/test_parts_standalone_v8/test_parts.js')\"",
    "copy:standalone:v8":                "npm run _copy:standalone:v8:parts && npm run _copy:standalone:v8:test_parts",
    "": ""
  },
  "watch": {
    "test:source:deno": {
      "patterns": [
        "source"
      ],
      "extensions": "js"
    },
    "start": {
      "patterns": [
        "source"
      ],
      "extensions": "js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/standard-software/partsjs.git"
  },
  "keywords": [],
  "author": "Satoshi Yamamoto <standard.software.net@gmail.com> (https://www.facebook.com/stndardsoftware/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/standard-software/parts.js/issues"
  },
  "homepage": "https://github.com/standard-software/partsjs#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "eslint": "^7.5.0",
    "eslint-config-google": "^0.14.0",
    "fs-extra": "^9.0.1",
    "jest": "^25.5.4",
    "moment": "^2.27.0",
    "npm-watch": "^0.7.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {}
}
