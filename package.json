{
  "private": false,
  "name": "@standard-software/parts",
  "version": "9.2.0",
  "description": "JavaScript Code Parts",
  "main": "./release/parts.js",
  "scripts": {
    "babel":                  "babel --config-file ./config/babel.config.js ./source --out-dir ./build/babel_webpack/babel",
    "webpack:source":         "webpack --config ./config/webpack_source.config.js",
    "webpack:test":           "webpack --config ./config/webpack_test.config.js",
    "webpack:source:forwsh":  "webpack --config ./config/webpack_source_forWsh.config.js",
    "webpack:test:forwsh":    "webpack --config ./config/webpack_test_forWsh.config.js",
    "copy:release":           "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/parts.js', './release/parts.js')\"",
    "_copy:release:forwsh0":    "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack_forWsh/parts.js', './release_wsh/parts.js')\"",
    "_copy:release:forwsh1":    "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack_forWsh/1.parts.js', './release_wsh/1.parts.js')\"",
    "_copy:release:forwsh2":    "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack_forWsh/2.parts.js', './release_wsh/2.parts.js')\"",
    "copy:release:forwsh":    "npm run _copy:release:forwsh0 && npm run _copy:release:forwsh1 && npm run _copy:release:forwsh2",
    "clear:babel":            "node -e \"require('fs-extra').removeSync('./build/babel_webpack/babel/')\"",
    "clear:webpack":          "node -e \"require('fs-extra').removeSync('./build/babel_webpack/webpack/')\"",
    "clear:webpack:forwsh":   "node -e \"require('fs-extra').removeSync('./build/babel_webpack/webpack_forWsh/')\"",
    "clear:build":            "npm run clear:babel && npm run clear:webpack && npm run clear:webpack:forwsh",
    "build:babel":            "npm run babel",
    "build:webpack":          "npm run webpack:source && npm run webpack:test",
    "build:webpack:forwsh":   "npm run webpack:source:forwsh && npm run webpack:test:forwsh",
    "test:source:deno:1":     "deno run --allow-write ./test/source/test_source_deno_1.js",
    "test:source:deno:2":     "deno run --allow-write ./test/source/test_source_deno_2.js",
    "test:source:deno:3":     "deno run --allow-write ./test/source/test_source_deno_3.js",
    "test:source:deno":       "npm run test:source:deno:1 && npm run test:source:deno:2 && npm run test:source:deno:3",
    "test:babel:node":        "node ./test/build/babel_webpack/babel/test_babel_webpack_babel_1.js && node ./test/build/babel_webpack/babel/test_babel_webpack_babel_2.js",
    "test:webpack:node":      "node ./test/build/babel_webpack/webpack/test_babel_webpack_webpack_1.js && node ./test/build/babel_webpack/webpack/test_babel_webpack_webpack_2.js",
    "test:webpack:wsh":       "cscript ./test/release/wsh/test.wsf",
    "start":                  "npm run test:source:deno && npm run build:babel && npm run test:babel:node && npm run build:webpack && npm run test:webpack:node && npm run copy:release",
    "start:wsh":              "npm run test:source:deno && npm run build:babel && npm run test:babel:node && npm run build:webpack:forwsh && npm run test:webpack:wsh && npm run copy:release:forwsh",
    "start:jest":             "npm start && jest",
    "watch:test:source:deno": "npm-watch test:source:deno",
    "watch:start":            "npm-watch start",
    "eslint":                 "eslint",
    "eslint:fix":             "eslint --fix ./source/*.js ./source/*/*.js",
    "jest":                   "jest",
    "__copy:sheet:rhino:parts":           "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/parts.js', './test/release/clasp/test_parts_sheet_Rhino/parts.js')\"",
    "__copy:sheet:rhino:test_parts":      "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/test_parts.js', './test/release/clasp/test_parts_sheet_Rhino/test_parts.js')\"",
    "_copy:sheet:rhino":                  "npm run __copy:sheet:rhino:parts && npm run __copy:sheet:rhino:test_parts",
    "__copy:sheet:v8:parts":              "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/parts.js', './test/release/clasp/test_parts_sheet_v8/parts.js')\"",
    "__copy:sheet:v8:test_parts":         "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/test_parts.js', './test/release/clasp/test_parts_sheet_v8/test_parts.js')\"",
    "_copy:sheet:v8":                     "npm run __copy:sheet:v8:parts && npm run __copy:sheet:v8:test_parts",
    "_copy:sheet":                        "npm run _copy:sheet:rhino && npm run _copy:sheet:v8",
    "__copy:standalone:rhino:parts":      "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/parts.js', './test/release/clasp/test_parts_standalone_Rhino/parts.js')\"",
    "__copy:standalone:rhino:test_parts": "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/test_parts.js', './test/release/clasp/test_parts_standalone_Rhino/test_parts.js')\"",
    "_copy:standalone:rhino":             "npm run __copy:standalone:rhino:parts && npm run __copy:standalone:rhino:test_parts",
    "__copy:standalone:v8:parts":         "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/parts.js', './test/release/clasp/test_parts_standalone_v8/parts.js')\"",
    "__copy:standalone:v8:test_parts":    "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/test_parts.js', './test/release/clasp/test_parts_standalone_v8/test_parts.js')\"",
    "_copy:standalone:v8":                "npm run __copy:standalone:v8:parts && npm run __copy:standalone:v8:test_parts",
    "_copy:standalone":                   "npm run _copy:standalone:rhino && npm run _copy:standalone:v8",
    "clasp:copy":                         "npm run _copy:sheet && npm run _copy:standalone",
    "_push:sheet:rhino":                  "cd ./test/release/clasp/test_parts_sheet_Rhino/ && clasp push",
    "_push:sheet:v8":                     "cd ./test/release/clasp/test_parts_sheet_v8/ && clasp push",
    "_push:sheet":                        "npm run _push:sheet:rhino && npm run _push:sheet:v8",
    "_push:standalone:rhino":             "cd ./test/release/clasp/test_parts_standalone_Rhino/ && clasp push",
    "_push:standalone:v8":                "cd ./test/release/clasp/test_parts_standalone_v8/ && clasp push",
    "_push:standalone":                   "npm run _push:standalone:rhino && npm run _push:standalone:v8",
    "clasp:push":                         "npm run _push:sheet && npm run _push:standalone",
    "_delete:sheet:rhino:parts":          "node -e \"require('fs-extra').removeSync('./test/release/clasp/test_parts_sheet_Rhino/parts.js')\"",
    "_delete:sheet:rhino:test_parts":     "node -e \"require('fs-extra').removeSync('./test/release/clasp/test_parts_sheet_Rhino/test_parts.js')\"",
    "_delete:sheet:rhino":                "npm run _delete:sheet:rhino:parts && npm run _delete:sheet:rhino:test_parts",
    "_delete:sheet:v8:parts":             "node -e \"require('fs-extra').removeSync('./test/release/clasp/test_parts_sheet_v8/parts.js')\"",
    "_delete:sheet:v8:test_parts":        "node -e \"require('fs-extra').removeSync('./test/release/clasp/test_parts_sheet_v8/test_parts.js')\"",
    "_delete:sheet:v8":                   "npm run _delete:sheet:v8:parts && npm run _delete:sheet:v8:test_parts",
    "_delete:sheet":                      "npm run _delete:sheet:rhino && npm run _delete:sheet:v8",
    "_delete:standalone:rhino:parts":     "node -e \"require('fs-extra').removeSync('./test/release/clasp/test_parts_standalone_Rhino/parts.js')\"",
    "_delete:standalone:rhino:test_parts":"node -e \"require('fs-extra').removeSync('./test/release/clasp/test_parts_standalone_Rhino/test_parts.js')\"",
    "_delete:standalone:rhino":           "npm run _delete:standalone:rhino:parts && npm run _delete:standalone:rhino:test_parts",
    "_delete:standalone:v8:parts":        "node -e \"require('fs-extra').removeSync('./test/release/clasp/test_parts_standalone_v8/parts.js')\"",
    "_delete:standalone:v8:test_parts":   "node -e \"require('fs-extra').removeSync('./test/release/clasp/test_parts_standalone_v8/test_parts.js')\"",
    "_delete:standalone:v8":              "npm run _delete:standalone:v8:parts && npm run _delete:standalone:v8:test_parts",
    "_delete:standalone":                 "npm run _delete:standalone:rhino && npm run _delete:standalone:v8",
    "clasp:delete":                       "npm run _delete:sheet && npm run _delete:standalone",
    "clasp:release":                      "npm run clasp:copy && npm run clasp:push && npm run clasp:delete",
    "_copy:docs:parts":                   "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/parts.js', './docs/test/web/parts.js')\"",
    "_copy:docs:test_parts":              "node -e \"require('fs').copyFileSync('./build/babel_webpack/webpack/test_parts.js', './docs/test/web/test_parts.js')\"",
    "copy:docs":                          "npm run _copy:docs:parts && npm run _copy:docs:test_parts",
    "": ""
  },
  "watch": {
    "test:source:deno": {
      "patterns": [
        "source"
      ],
      "extensions": "js"
    },
    "start": {
      "patterns": [
        "source"
      ],
      "extensions": "js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/standard-software/partsjs.git"
  },
  "keywords": [],
  "author": "Satoshi Yamamoto <standard.software.net@gmail.com> (https://www.facebook.com/stndardsoftware/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/standard-software/parts.js/issues"
  },
  "homepage": "https://github.com/standard-software/partsjs#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "eslint": "^7.5.0",
    "eslint-config-google": "^0.14.0",
    "fs-extra": "^9.0.1",
    "jest": "^25.5.4",
    "moment": "^2.27.0",
    "npm-watch": "^0.7.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {}
}
